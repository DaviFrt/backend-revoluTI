generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Address {
  id     String @id @default(uuid())
  street String
  city   String
  state  String
  zip    String

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  @@map("addresses")
}
